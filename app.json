{
  "name": "Plaid Starter App",
  "description": "Boilerplate Plaid App using Node.js",
  "repository": "https://github.com/austinbgibbons/goldmine_leak_public_disclosure.git.git",
  "logo": "https://plaid.com/images/contact/press/logo-vertical-white.png",
  "keywords": ["node", "express", "plaid"],
  "addons": [
    {
      "plan": "mongolab:sandbox",
      "as": "MONGO"
    }
  ],
  "env": {
    "PLAID_ENV": {
      "description": "Plaid environment to test against: sandbox, development, production",
      "value": "sandbox"
    },
    "PLAID_CLIENT_ID": {
      "description": "Your Plaid client_id",
      "required": true
    },
    "PLAID_SECRET": {
      "description": "Your Plaid secret for the environment you are testing against",
      "required": true
    },
    "PLAID_PUBLIC_KEY": {
      "description": "Your Plaid public_key",
      "required": true
    },
    "TRANSACTION_WEBHOOK_URL": {
      "description": "Webhook url for transaction updates from Plaid",
      "required": true
    }
  }
}
